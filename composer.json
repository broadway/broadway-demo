    "require": {
        "php": ">=7.2",
        "ext-pdo_sqlite": "*",
        "symfony/symfony": "^4.4",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.4",
        "symfony/monolog-bundle": "^3.1",
        "broadway/broadway-bundle": "~0.5",
        "broadway/event-store-dbal": "~0.4",
        "doctrine/doctrine-bundle": "^1.8",
        "qandidate/stack-request-id": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BroadwayDemo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BroadwayDemo\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "broadway/coding-standard": "^1.0"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
